    "gatsby": "^2.20.25",
    "gh-pages": "^2.2.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "yargs-parser": "^18.1.3"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "homepage": "https://github.com/contentful-userland/gatsby-contentful-starter#readme",
  "keywords": [
